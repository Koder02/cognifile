{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
  "postinstall": "node scripts/copy-pdfjs-worker.js",
  "dev": "node scripts/gen-data-index.js && node scripts/copy-pdfjs-worker.js && vite",
  "build": "node scripts/gen-data-index.js && node scripts/copy-pdfjs-worker.js && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "semantic:start": "cd server/semantic_search && uvicorn app:app --host 0.0.0.0 --port 8001 --reload",
    "semantic:index": "node --loader ts-node/esm scripts/index-docs-for-search.ts"
  },
  "dependencies": {
    "@huggingface/inference": "^2.8.1",
    "@supabase/supabase-js": "^2.57.4",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^3.21.0",
    "@tensorflow/tfjs-backend-cpu": "^3.21.0",
    "@tensorflow/tfjs-backend-webgl": "^3.21.0",
    "lucide-react": "^0.344.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.149",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
